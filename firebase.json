{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "destination": "/services/dev/[slug]/index.html",
        "regex": "^/services/dev/([^/]+?)(?:/)?$"
      },
      {
        "destination": "/services/onprem/[slug]/index.html",
        "regex": "^/services/onprem/([^/]+?)(?:/)?$"
      }
    ]
  }
}
